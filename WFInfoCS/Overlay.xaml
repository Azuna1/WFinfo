        <Label x:Name="Part_Name_Copy1" Content="14.5" HorizontalAlignment="Center" Margin="76,88,148,0" VerticalAlignment="Top" Foreground="#FFB1D0D9"/>
        <Label x:Name="Part_Name_Copy2" Content="14.5" HorizontalAlignment="Center" Margin="152,88,72,0" VerticalAlignment="Top" Foreground="#FFB1D0D9"/>
        <Image HorizontalAlignment="Left" Height="12" Margin="140,95.5,0,0" VerticalAlignment="Top" Width="12" Source="Resources/ducat_w.gif"/>
